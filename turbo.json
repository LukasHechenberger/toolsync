{
  "tasks": {
    "build": {
      "outputs": [
        "README.md",
        ".next/**",
        "!.next/cache/**",
        "out/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ]
    },
    "//#check:format": {},
    "check": {
      "dependsOn": [
        "//#check:format",
        "check:types",
        "check:exports"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check:types": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": [
        "test:transit"
      ]
    },
    "test:transit": {
      "dependsOn": [
        "^test:transit"
      ]
    },
    "check:exports": {
      "dependsOn": [
        "build"
      ]
    }
  },
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui"
}
