{
  "name": "@toolsync/cli",
  "version": "0.2.3",
  "description": "The toolsync command line interface",
  "homepage": "https://toolsync.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/toolsync.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "default": "./out/index.js"
    },
    "./commands/init": {
      "types": "./out/commands/init.d.ts",
      "import": "./out/commands/init.js",
      "default": "./out/commands/init.js"
    }
  },
  "bin": {
    "toolsync": "./out/bin.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "update-readme": "node ./node_modules/@toolsync/template/out/bin.js README.md cli-usage \"\\`\\`\\`\n$(LOG_LEVEL=silent node ./out/bin.js --help)\n\\`\\`\\`\""
  },
  "dependencies": {
    "@toolsync/core": "workspace:^",
    "@toolsync/logger": "workspace:^",
    "commander": "^14.0.0",
    "node-plop": "^0.32.0",
    "terminal-link": "^4.0.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@toolsync/builtin": "workspace:^",
    "@toolsync/template": "workspace:*",
    "@types/node": "^22.15.30",
    "tsup": "^8.5.0"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
