    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@devtools/logger": "workspace:^",
    "@manypkg/get-packages": "^3.0.0",
    "@manypkg/tools": "^2.0.0",
    "debug": "^4.4.1",
    "deepmerge-ts": "^7.1.5"
