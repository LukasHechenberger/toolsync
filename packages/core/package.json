{
  "name": "@toolsync/core",
  "version": "0.0.0",
  "description": "Core functionality used in @toolsync/cli etc",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukasHechenberger/toolsync",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Lukas Hechenberger <npm@ls-age.com> (https://ls-age.com/)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "default": "./out/index.js"
    },
    "./plugins": {
      "types": "./out/plugins.d.ts",
      "import": "./out/plugins.js",
      "default": "./out/plugins.js"
    },
    "./types": {
      "types": "./out/types.d.ts",
      "import": "./out/types.js",
      "default": "./out/types.js"
    }
  },
  "main": "out/index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@manypkg/get-packages": "^3.0.0",
    "@manypkg/tools": "^2.0.0",
    "@toolsync/logger": "workspace:^",
    "debug": "^4.4.1",
    "deepmerge-ts": "^7.1.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.15.29",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
