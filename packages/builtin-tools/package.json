{
  "name": "@toolsync/builtin",
  "version": "0.4.1",
  "description": "Default toolsync plugins available",
  "homepage": "https://toolsync.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/toolsync.git",
    "directory": "packages/builtin-tools"
  },
  "license": "MIT",
  "author": "Lukas Hechenberger <npm@ls-age.com> (https://ls-age.com/)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js"
    },
    "./ignore-sync": {
      "types": "./out/ignore-sync/index.d.ts",
      "default": "./out/ignore-sync/index.js"
    },
    "./tools.json": "./out/tools.json",
    "./vscode": {
      "types": "./out/vscode/index.d.ts",
      "default": "./out/vscode/index.js"
    },
    "./prettier": {
      "types": "./out/prettier/index.d.ts",
      "default": "./out/prettier/index.js"
    },
    "./github-actions": {
      "types": "./out/github-actions/index.d.ts",
      "default": "./out/github-actions/index.js"
    },
    "./pnpm": {
      "types": "./out/pnpm/index.d.ts",
      "default": "./out/pnpm/index.js"
    },
    "./package-readme": {
      "types": "./out/package-readme/index.d.ts",
      "default": "./out/package-readme/index.js"
    },
    "./package-meta": {
      "types": "./out/package-meta/index.d.ts",
      "default": "./out/package-meta/index.js"
    },
    "./turbo": {
      "types": "./out/turbo/index.d.ts",
      "default": "./out/turbo/index.js"
    },
    "./publint": {
      "types": "./out/publint/index.d.ts",
      "default": "./out/publint/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "check:exports": "publint",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@toolsync/core": "workspace:^",
    "@toolsync/template": "workspace:^",
    "markdown-table": "^3.0.4",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@turbo/types": "^2.5.4",
    "@types/node": "^22.15.30",
    "ignore-sync": "^8.0.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "ignore-sync": "^8.0.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12"
  },
  "peerDependenciesMeta": {
    "ignore-sync": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "publint": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
