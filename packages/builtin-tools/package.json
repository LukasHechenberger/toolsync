{
  "name": "@devtools/builtin",
  "version": "0.0.0",
  "type": "module",
  "description": "Default devtools available",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js"
    },
    "./ignore-sync": {
      "types": "./out/ignore-sync/index.d.ts",
      "default": "./out/ignore-sync/index.js"
    },
    "./vscode": {
      "types": "./out/vscode/index.d.ts",
      "default": "./out/vscode/index.js"
    },
    "./prettier": {
      "types": "./out/prettier/index.d.ts",
      "default": "./out/prettier/index.js"
    },
    "./github-actions": {
      "types": "./out/github-actions/index.d.ts",
      "default": "./out/github-actions/index.js"
    },
    "./pnpm": {
      "types": "./out/pnpm/index.d.ts",
      "default": "./out/pnpm/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "author": "Lukas Hechenberger <npm@ls-age.com> (https://ls-age.com/)",
  "license": "MIT",
  "dependencies": {
    "@devtools/core": "workspace:^"
  },
  "devDependencies": {
    "ignore-sync": "^8.0.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "yaml": "^2.8.0"
  },
  "peerDependencies": {
    "ignore-sync": "^8.0.0",
    "prettier": "^3.5.3",
    "yaml": "^2.8.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    },
    "ignore-sync": {
      "optional": true
    },
    "yaml": {
      "optional": true
    }
  }
}
