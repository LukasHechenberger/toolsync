{
  "name": "@toolsync/template",
  "version": "0.0.0",
  "description": "Update sections of source files",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukasHechenberger/toolsync",
    "directory": "packages/template"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/index.js"
    }
  },
  "bin": {
    "update-section": "./out/bin.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "update-readme": "node out/bin.js README.md cli-usage \"\\`\\`\\`\n$(node ./out/bin.js --help)\n\\`\\`\\`\""
  },
  "dependencies": {
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "tsup": "^8.5.0"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
